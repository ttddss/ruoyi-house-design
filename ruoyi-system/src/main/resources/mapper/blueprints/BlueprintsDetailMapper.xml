<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.blueprints.mapper.BlueprintsDetailMapper">
    
    <resultMap type="BlueprintsDetail" id="BlueprintsDetailResult">
        <result property="id"    column="id"    />
        <result property="blueprintsId"    column="blueprints_id"    />
        <result property="introduction"    column="introduction"    />
        <result property="fileUrl"    column="file_url"    />
        <result property="previewImageUrl"    column="preview_image_url"    />
        <result property="introductionImageUrl"    column="introduction_image_url"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="remark"    column="remark"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectBlueprintsDetailVo">
        select id, blueprints_id, introduction, file_url, del_flag, remark, create_by, create_time, update_by, update_time from blueprints_detail
    </sql>

    

</mapper>